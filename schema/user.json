{
  "$schema": "http://json-schema.org/schema#",
  "$id": "http://posting.ly/schemas/address.json",
  "description": "Base user of Postingly",
  "type": "object",
  "properties": {
    "email": {
      "type": "string",
      "format": "email"
    },
    "name": {
      "type": "string"
    },
    "created_at": {
      "type": "string",
      "format": "date-time"
    }
  },
  "required": [
    "email"
  ]
}